<MainLayout
  title="Contact | Kiss D'Kiss"
  lang="fr"
  structuredData={{
    "@context": "https://schema.org",
    "@type": "WebPage",
    "@id": "https://kissdkiss.com/fr/contact",
    url: "https://kissdkiss.com/fr/contact",
    name: "Nous Contacter | ScrewFast",
    description:
      "Empowering students from around the world to pursue their dreams of studying in the U.S.",
    isPartOf: {
      "@type": "WebSite",
      url: "https://kissdkiss.com/fr/",
      name: "ScrewFast",
      description:
        "Empowering students from around the world to pursue their dreams of studying in the U.S.",
    },
    inLanguage: "fr",
  }}
>
  <ContactSection />
</MainLayout>
